                                    </div>



                                    <script>
                                        // JavaScript to update the custom progress bar width
                                        const customProgressBar = document.getElementById('normal-progress');
                                        const customProgressBar1 = document.getElementById('easy-progress');
                                        const customProgressBar2 = document.getElementById('medium-progress');
                                        const customProgressBar3 = document.getElementById('hard-progress');
                                        const customProgressBarObj = document.getElementById('normal-progress-info');
                                        const customProgressBarObj1 = document.getElementById('easy-progress-info');
                                        const customProgressBarObj2 = document.getElementById('medium-progress-info');
                                        const customProgressBarObj3 = document.getElementById('hard-progress-info');
                        
