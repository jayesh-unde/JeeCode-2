                                    <div style="display: none;" id="normal-progress-info"><%= Avg %></div>
                                    <div class="progress mb-3" style="height: 12px">
                                        <div class="progress-bar bg-primary" id="normal-progress" role="progressbar" style="width: 0%"
                                            aria-valuenow="72" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                    
                                    

                                    <small>Easy &nbsp;<%= easySolved %>/<%= easy %></small>
                                    <div class="progress mb-3" style="height: 12px">
                                        <div style="display: none;" id="easy-progress-info"><%= easyAvg %></div>
                                        <div class="progress-bar bg-success" id="easy-progress" role="progressbar" style="width:0%"
                                            aria-valuenow="72" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                    <small>Medium &nbsp;<%= mediumSolved %>/<%= medium %> </small>

